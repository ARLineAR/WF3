{% extends "layout.html.twig" %}

{% block content %}

    <table>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
        {% for category in categories %}
            <tr>
                <td>{{ category.id }}</td>
                <td>{{ category.name }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
